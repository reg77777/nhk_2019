<launch>

  <!-- Arcuion1 node -->
    <node pkg="rosserial_python" type="serial_node.py" name="arduino1">
      <param name="port" value="/dev/ttyACM0" />
    </node>

  <!-- Arcuion2 node -->
    <node pkg="rosserial_python" type="serial_node.py" name="arduino2">
      <param name="port" value="/dev/ttyACM1" />
    </node>

</launch>
